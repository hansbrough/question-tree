{{#SCREEN}}
<div class="question-set-container {{id}}">

  <p class="weight-book medium-large overview">
    Great work finishing the quiz! Here's how you did.
  </p>

  {{!-- DESKTOP --}}
  <div class="criteria-header">
    <p class="column-name genus col-1-6 small-large">Genus</p><p class="column-name example-img col-1-6 small-large">Know it?</p><p class="column-name correct col-1-6 small-large"># Correct</p>
  </div>

  {{#each results}}
  <div class="data-row {{stripes @index}}{{#if @first}} first{{/if}}{{#if @last}} last{{/if}}">
    <p class="criterion col-1-6">{{displayName}}</p>
    <p class="criterion-supported col-1-6"><img src="/assets/img/icons/mini-checkcircle-18px.svg"></p>
    <p class="criterion-count col-1-6">{{correct}}</p>
  </div>
  {{/each}}



